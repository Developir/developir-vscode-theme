	"semanticHighlighting": true,
	"semanticTokenColors": {
		"number": 		"#444444",
		"comment": 		"#a5a5a5",
		"keyword": 		"#245ed1",
		"operator": 	"#245ed1",
		"function": 	"#e20505",
		"method": 		"#e20505",
		"variable": 	"#ff9500",
		"parameter": 	"#eab701",
		"property": 	"#A05A48",
		"enumMember": 	"#A05A48",
		"string": 		"#1d990a",
		"interface": 	"#7030a0",
		"type": 		"#7030a0",
		"typeParameter": 	"#df0ab8",
		"enum": 			"#df0ab8",
		"namespace": 		"#df0ab8",
		"class": 			"#df0ab8",
		"class.defaultLibrary": "#df0ab8",
	},
	"tokenColors": [
		/*---------- Black ----------*/
		{
			"name": "number",
			"scope": [
				"constant.numeric",
			],
			"settings": {"foreground": "#444444"}
		},
		{
			"name": "signs = => {} () [] <> : , ;",
			"scope": [
				"punctuation",
				"keyword.operator.assignment",
				"storage.type.function.arrow",
				"keyword.operator.type.annotation",
				"meta.brace.round",
				"meta.brace.square",
				"keyword.operator.combinator.css",
				"entity.name.tag.wildcard",
			],
			"settings": {"foreground": "#444444"}
		},
		{
			"name": "css property value",
			"scope": [
				"support.constant.property-value",
				"meta.property-value.css",
			],
			"settings": {"foreground": "#444444"}
		},
		{
			"name": "css number",
			"scope": [
				"constant.numeric.css",
			],
			"settings": {"foreground": "#444444"}
		},
		{
			"name": "css unit",
			"scope": [
				"keyword.other.unit",
			],
			"settings": {"foreground": "#444444"}
		},
		{
			"name": "css hex color",
			"scope": [
				"constant.other.color.rgb-value.hex",
			],
			"settings": {"foreground": "#444444"}
		},
		{
			"name": "import alias in typescript",
			"scope": [
				"meta.import variable.other.readwrite"
			],
			"settings": {"foreground": "#444444"}
		},
		/*---------- Grey ----------*/
		{
			"name": "comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {"foreground": "#a5a5a5"}
		},
		/*---------- Blue ----------*/
		{
			"name": "keyword: if-esle, for, return, try, catch, throw, switch-case, import, from, as, export, while, continue, await,  ",
			"scope": [
				"keyword.control",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "operator: + - * /, == === > < <= >= ! !==, ++, %, ... spread, && ||, ??, in, new, instanceof, | union type, ternary ? : , keyof type, ? optional type, delete, for of in, optional property type",
			"scope": [
				"keyword.operator",
				"keyword.other",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "keyword 2: var/let/const, function, clas, constructor, type, interface, enum, module, get/set ",
			"scope": [
				"storage.type",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "keyword 3: extends, public, private, protected, static, async, implements, declare, abstract",
			"scope": [
				"storage.modifier",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "super() keyword in class & super.properyName",
			"scope": [
				"variable.language.super",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "Optional chaining user?.city",
			"scope": [
				"punctuation.accessor.optional",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "template literal: `Hello ${name} welcome",
			"scope": [
				"punctuation.definition.template-expression",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "link in markdown syntax",
			"scope": [
				"markup.underline.link.markdown",
			],
			"settings": {"foreground": "#245ed1"}
		},
		{
			"name": "triple slash directive: ///<reference>",
			"scope": [
				"comment.line.triple-slash.directive.ts punctuation.definition.comment",
			],
			"settings": {"foreground": "#245ed1"}
		},
		/*---------- Red ----------*/
		{
			"name": "function, method, css function",
			"scope": [
				"entity.name.function",
			],
			"settings": {"foreground": "#e20505"}
		},
		{
			"name": "other functions: css: var(), rgba(), scale(), calc(), min()",
			"scope": [
				"support.function",
			],
			"settings": {"foreground": "#e20505"}
		},
		{
			"name": "css !important",
			"scope": [
				"keyword.other.important",
			],
			"settings": {"foreground": "#e20505"}
		},
		/*---------- Green ----------*/
		{
			"name": "string: single and double quotes, template literal",
			"scope": [
				"string.quoted",
				"string.template",
				"string.regexp",
				"string.unquoted",
				"string.interpolated",
				"string.other",
			],
			"settings": {"foreground": "#1d990a"}
		},
		{
			"name": "css font names",
			"scope": [
				"support.constant.font-name.css",
			],
			"settings": {"foreground": "#1d990a"}
		},
		{
			"name": "css attribute value input[title='text']",
			"scope": [
				"meta.attribute-selector.scss",
			],
			"settings": {"foreground": "#1d990a"}
		},
		/*---------- Orange ----------*/
		{
			"name": "variables",
			"scope": [
				"variable.other.readwrite",
				"variable.other.constant",
				"variable.other.object",
				"variable.other.php",
			],
			"settings": {"foreground": "#ff9500"}
		},
		{
			"name": "language variables: this, self",
			"scope": [
				"variable.language",
			],
			"settings": {"foreground": "#ff9500"}
		},
		{
			"name": "css, sass, less variables",
			"scope": [
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"variable.argument.css",
			],
			"settings": {"foreground": "#ff9500"}
		},
		{
			"name": "& in sass/less",
			"scope": [
				"entity.name.tag.reference.scss",
				"entity.other.attribute-name.parent-selector punctuation.definition.entity.css"
			],
			"settings": {"foreground": "#ff9500"}
		},
		{
			"name": "$ sign in variable in php",
			"scope": [
				"punctuation.definition.variable.php",
			],
			"settings": {"foreground": "#ff9500"}
		},
		{
			"name": "variable in jsdoc comment: /** @param {string} title */",
			"scope": [
				"variable.other.jsdoc",
			],
			"settings": {"foreground": "#ff9500"}
		},
		{
			"name": "variable in shell script like .env NEXT_PUBLIC_BASE_URL",
			"scope": [
				"variable.other.assignment.shell",
			],
			"settings": {"foreground": "#ff9500"}
		},
		{
			"name": "constant variable in php",
			"scope": [
				"constant.other.php",
			],
			"settings": {"foreground": "#ff9500"}
		},
		/*---------- Yellow ----------*/
		{
			"name": "parameters in function/constructor/methods, css animation name",
			"scope": [
				"variable.parameter",
			],
			"settings": {"foreground": "#eab701"}
		},
		{
			"name": "css pseudo class/element",
			"scope": [
				"entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element",
			],
			"settings": {"foreground": "#eab701"}
		},
		{
			"name": "css at rule @media name or @tailwind base",
			"scope": [
				"support.constant.media.css",
				"meta.at-rule.header.css"
			],
			"settings": {"foreground": "#eab701"}
		},
		{
			"name": "number in tsx attribute: <a width={100}>",
			"scope": [
				"meta.tag.attributes.tsx constant.numeric.decimal.tsx",
			],
			"settings": {"foreground": "#eab701"}
		},
		/*---------- Brown ----------*/
		{
			"name": "property key in object",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"meta.object-literal.key"
			],
			"settings": {"foreground": "#A05A48"}
		},
		{
			"name": "string property key {'id': 1}",
			"scope": [
				"meta.object-literal.key string.quoted",
			],
			"settings": {"foreground": "#A05A48"}
		},
		{
			"name": "css property key",
